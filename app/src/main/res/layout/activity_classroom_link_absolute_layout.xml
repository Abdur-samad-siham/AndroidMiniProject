<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClassroomLinkAbsoluteLayout">
    <Button
        android:id="@+id/PreviousID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Previous Layout"
        android:layout_x="10dp"
        android:layout_y="10dp"
        android:visibility="visible"
        />
    <Button
        android:id="@+id/NextID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next Layout"
        android:layout_x="200dp"
        android:layout_y="10dp"
        android:visibility="visible"
        />

    <Button
        android:id="@+id/classroomID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GOOGLE CLASSROOM"
        android:layout_x="100dp"
        android:layout_y="150dp"
        android:visibility="visible"
        />
    <Button
        android:id="@+id/austID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="             aust.edu        "
        android:layout_x="100dp"
        android:layout_y="250dp"
        android:visibility="visible"
        />
    <Button
        android:id="@+id/iumsID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="                  IUMS             "
        android:layout_x="100dp"
        android:layout_y="350dp"
        android:visibility="visible"
        />
    <Button
        android:id="@+id/GmailID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="              GMAIL             "
        android:layout_x="100dp"
        android:layout_y="450dp"
        android:visibility="visible"
        />
<WebView
    android:id="@+id/webviewID"
    android:layout_width="match_parent"
    android:layout_height="match_parent"/>

</AbsoluteLayout>